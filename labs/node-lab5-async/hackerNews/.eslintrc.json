{
  "extends": "google",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
    "env": {
        "node": true
    },

    "globals": {
        "React": true
    },

    "ecmaFeatures": {
        "jsx": true
    },

    "plugins": [
        "react"
    ],
"rules": {
    "max-len": [2, 120, 8]
  }
}


